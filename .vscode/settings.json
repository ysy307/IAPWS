{
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.renderLineHighlight": "all",
    "editor.dragAndDrop": false,
    "editor.acceptSuggestionOnEnter": "off",
    "files.autoSave": "onWindowChange",
    "files.associations": {
        "*.FOR": "FortranFreeForm",
        "*.f95": "FortranFreeForm",
        "*.f03": "FortranFreeForm",
        "*.f90": "FortranFreeForm",
        "*.py": "python",
        "*.vtu": "xml",
        ".fortls": "json",
        ".VERSION": "plaintext",
        "*.F90": "FortranFreeForm",
        "complex": "cpp",
        "algorithm": "cpp",
        "memory": "cpp",
        "numeric": "cpp",
        "cctype": "cpp",
        "cfenv": "cpp",
        "cinttypes": "cpp",
        "clocale": "cpp",
        "compare": "cpp",
        "csetjmp": "cpp",
        "csignal": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "memory_resource": "cpp",
        "random": "cpp",
        "source_location": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "new": "cpp",
        "typeinfo": "cpp",
        "*.in": "cpp"
    },
    "fortran.fortls.path": "/usr/local/bin/fortls",
    "fortran.linter.extraArgs": [
        "-fpp",
        "-module",
        "/workspaces/FTDSS/CMakeBuild/.mod",
        "-module",
        "/opt/intel/oneapi/mkl/latest/include",
        "-module",
        "/opt/intel/oneapi/mpi/latest/include",
        "-D_MKL",
        "-D_MPI"
    ],
    "fortran.linter.compiler": "ifx",
    "fortran.linter.compilerPath": "/opt/intel/oneapi/compiler/latest/bin/ifx",
    "fortran.linter.modOutput": "/workspaces/IAPWS/CMakeBuild/.mod/",
    "fortran.fortls.nthreads": 16,
    "fortran.fortls.preprocessor.suffixes": [
        ".F90"
    ],
    "fortran.fortls.directories": [
        "${workspaceFolder}/src",
    ],
    "fortran.fortls.excludeDirectories": [
        "${workspaceFolder}/.archive",
        "${workspaceFolder}/.devcontainer",
        "${workspaceFolder}/.github",
        "${workspaceFolder}/.vscode",
        "${workspaceFolder}/cmake",
        "${workspaceFolder}/docs",
        "${workspaceFolder}/fpm",
        "${workspaceFolder}/Inout",
        "${workspaceFolder}/log",
        "${workspaceFolder}/third_party",
        "${workspaceFolder}/test"
    ],
    "fortran.fortls.notifyInit": true,
    "fortran.fortls.sortKeywords": true,
    "fortran.linter.includePaths": [
        "/workspaces/IAPWS/CMakeBuild/.mod",
        "/opt/intel/oneapi/mkl/latest/include",
        "/opt/intel/oneapi/mpi/latest/include/mpi",
    ],
    "fortran.fortls.disableDiagnostics": true,
    "fortran.formatting.formatter": "fprettify",
    "fortran.formatting.fprettifyArgs": [
        "--indent",
        "4",
        "--whitespace",
        "4",
        "--whitespace-comma",
        "true",
        "--whitespace-assignment",
        "true",
        "--whitespace-relational",
        "true",
        "--whitespace-logical",
        "true",
        "--whitespace-plusminus",
        "true",
        "--whitespace-multdiv",
        "true",
        "--whitespace-print",
        "true",
        "--whitespace-intrinsics",
        "true",
        "--whitespace-decl",
        "true",
        "--whitespace-type",
        "false",
        "--strip-comments",
        "--case",
        "1",
        "1",
        "1",
        "1",
        "--line-length",
        "256",
        "--silent",
        "--enable-decl",
        "--whitespace-decl",
        "true",
        "--strict-indent"
    ],
    "fortran.preferredCase": "lowercase",
    "[FortranFreeForm]": {
        "files.autoGuessEncoding": true,
        "files.encoding": "utf8"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.tmp.drive*": true
    },
    "workbench.colorCustomizations": {
        "editor.background": "#2e282c"
    },
    "editor.tokenColorCustomizations": {
        "comments": {
            "foreground": "#876487",
            "fontStyle": "italic",
        },
        "functions": "#99bcf0",
        "keywords": "#dbf",
        "numbers": "#f99",
        "strings": "#df8",
        "types": "#fb6",
        "variables": "#ddd",
    },
    "editor.rulers": [
        132
    ],
    "containers.commands.attach": "${containerCommand} exec -it ${containerId} zsh",
    "python.analysis.include": [
        "scripts/*"
    ],
}